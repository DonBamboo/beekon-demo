{
  "name": "q_ide_default",
  "description": "Default agent configuration",
  "prompt": "",
  "mcpServers": {
    "shadcn-ui": {
      "command": "npx",
      "args": [
        "@jpisnice/shadcn-ui-mcp-server"
      ],
      "timeout": 120000
    },
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "env": {},
      "timeout": 120000
    },
    "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest",
        "--isolated",
        "--storage-state={E:misc-repo/playwright-storage/store.json}"
      ]
    },
    "task-master-ai": {
      "command": "npx",
      "args": [
        "-y",
        "--package=task-master-ai",
        "task-master-ai"
      ]
    },
    "tavily-remote": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote",
        "https://mcp.tavily.com/mcp/?tavilyApiKey=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"
      ]
    },
    "clickup": {
      "command": "npx",
      "args": [
        "-y",
        "@chykalophia/clickup-mcp-server"
      ],
      "env": {
        "CLICKUP_API_TOKEN": "pk_44110364_3LAX3YW821YZTCUY0DA7AAKZJFPF3MBY"
      }
    }
  },
  "tools": [
    "fs_read",
    "execute_bash",
    "fs_write",
    "report_issue",
    "use_aws",
    "@shadcn-ui",
    "@sequential-thinking",
    "@playwright",
    "@task-master-ai",
    "@tavily-remote",
    "@clickup",
    "fsRead",
    "fsWrite",
    "fsReplace",
    "listDirectory",
    "fileSearch",
    "executeBash",
    "codeReview",
    "displayFindings"
  ],
  "toolAliases": {},
  "allowedTools": [
    "fs_read",
    "report_issue",
    "use_aws",
    "execute_bash",
    "fs_write",
    "fsRead",
    "listDirectory",
    "fileSearch",
    "codeReview",
    "displayFindings"
  ],
  "toolsSettings": {
    "use_aws": {
      "alwaysAllow": [
        {
          "preset": "readOnly"
        }
      ]
    },
    "execute_bash": {
      "alwaysAllow": [
        {
          "preset": "readOnly"
        }
      ]
    }
  },
  "resources": [
    "file://AmazonQ.md",
    "file://README.md",
    "file://.amazonq/rules/**/*.md"
  ],
  "hooks": {
    "agentSpawn": [],
    "userPromptSubmit": []
  },
  "useLegacyMcpJson": true
